{
  "name": "ron-web",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "npm run serve:ssr",
    "start:local": "run-script-os",
    "start:local:win32": "ng serve --open --proxy-config proxy.conf.local.json --configuration development",
    "start:local:darwin": "ng serve --open --proxy-config proxy.conf.local.json --configuration development",
    "start:default": "ng serve --open --proxy-config proxy.conf.local.json",
    "build": "ng build",
    "build:gitHubPage": "ng build --base-href /RonWeb/",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "mock-service:win32": "cd osprey-mock-service && start cmd /c osprey-mock-service -f api.raml -p 3000",
    "mock-service:darwin": "cd osprey-mock-service; osprey-mock-service -f api.raml -p 3000 &",
    "prestart:local:win32": "npm run mock-service:win32",
    "prestart:local:darwin": "npm run mock-service:darwin",
    "dev:ssr": "ng run RonWeb:serve-ssr",
    "serve:ssr": "node dist/RonWeb/server/main.js",
    "build:ssr": "ng build && ng run RonWeb:server",
    "prerender": "ng run RonWeb:prerender",
    "prepare": "husky install",
    "heroku-postbuild": "npm run prerender",
    "build:docker": "docker build -t ronweb . --no-cache",
    "killport": "npx kill-port --port 4200,3000"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^16.0.0",
    "@angular/common": "^16.0.0",
    "@angular/compiler": "^16.0.0",
    "@angular/core": "^16.0.0",
    "@angular/forms": "^16.0.0",
    "@angular/platform-browser": "^16.0.0",
    "@angular/platform-browser-dynamic": "^16.0.0",
    "@angular/platform-server": "^16.0.0",
    "@angular/router": "^16.0.0",
    "@ng-icons/core": "^23.3.0",
    "@ng-icons/heroicons": "^23.3.0",
    "@nguniversal/express-engine": "^16.0.0",
    "animate.css": "^4.1.1",
    "daisyui": "^2.51.5",
    "express": "^4.15.2",
    "lottie-web": "^5.11.0",
    "rxjs": "~7.8.0",
    "swiper": "^9.2.2",
    "tslib": "^2.3.0",
    "typed.js": "^2.0.15",
    "zone.js": "~0.13.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^16.0.0",
    "@angular/cli": "~16.0.0",
    "@angular/compiler-cli": "^16.0.0",
    "@nguniversal/builders": "^16.0.0",
    "@types/express": "^4.17.0",
    "@types/jasmine": "~4.3.0",
    "@types/node": "^14.15.0",
    "autoprefixer": "^10.4.14",
    "husky": "^8.0.3",
    "jasmine-core": "~4.6.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.0.0",
    "lint-staged": "^13.2.2",
    "osprey-mock-service": "^1.0.0",
    "postcss": "^8.4.23",
    "prettier": "^2.8.8",
    "pretty-quick": "^3.1.3",
    "run-script-os": "^1.1.6",
    "tailwindcss": "^3.3.2"
  },
  "lint-staged": {
    "*.{ts,js,vue}": [
      "eslint --fix"
    ],
    "*.{html,scss,css,vue}": [
      "prettier --write",
      "stylelint --fix"
    ]
  }
}
